{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["src", "package.json", "README.md"],
      "exclude": ["node_modules", "dist"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules|dist)"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "../interplanetary-players-docs/docs",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
        "default_theme": "dark",
        "base_url": "/interplanetary-players-docs/",
        "favicon": "https://mw-storage.fra1.cdn.digitaloceanspaces.com/default/logos/favicon.ico",
        "homepageTitle": "Interplanetary Players Documentation",
        "title": "<img src='https://mw-storage.fra1.cdn.digitaloceanspaces.com/default/logos/Maar-World-Logo.svg' class='logo' width='80' height='80'/> Interplanetary Players Dev Docs",
        "includeFilesListInHomepage": true,
        "menu": [
        {
            "title": "GitHub",
            "id": "github",
            "link": "https://github.com/maar34/interplanetary-players-docs"
        },
        {
            "title": "Maar World",
            "id": "website",
            "link": "https://maar.world/"
        }
        ],
        "meta": [
        {
            "name": "𝐵𝓇𝓊𝓃𝒶 𝒢𝓊𝒶𝓇𝓃𝒾𝑒𝓇𝒾",
            "content": "𝙼𝙰𝙰𝚁 𝚆𝙾𝚁𝙻𝙳"
        },
        {
            "name": "description",
            "content": "Official documentation for the Interplanetary Players project."
        }
        ],
        "search": true
    }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
        }
  }